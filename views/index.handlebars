<header>
  <img src="/images/staxx-green.svg" id="staxx-logo" alt="Staxx Logo">
</header>

<div class="signup">
  <div class="img-signin">
    <img src="/images/staxx-white.svg" id="staxx-logo-w" alt="Staxx Logo">
    <h2>// Sign In</h2>
  </div>
  <form>
    <div class="form-group">
      <label for="username">Username</label>
      <input type="text" id="username" placeholder="Username">
      <small>Must be unique.</small>
    </div>
    <div class="form-group2">
      <label for="userpassword">Password</label>
      <input type="password" id="userpassword" placeholder="Password">
    </div>
    <div class="form-group3">
      <label for="confirmpassword">Confirm Password</label>
      <input type="password" id="confirmpassword" placeholder="Enter Password Again">
    </div>
    <button type="submit" class="submit-btn" data_state="sign-in">Sign In</button>
  </form>
  <span id="create-popup">Create an account</span>
</div>

<footer>
  <a href="">Omega Giga X-Force Extreme</a>
</footer>


<script type="text/javascript" src="/js/index.js"></script>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>